<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.github.xudaojie.jdoc.model.AccountModel">
    <select id="get" parameterType="int" resultType="io.github.xudaojie.jdoc.model.AccountModel">
        select * from account where id = #{id}
    </select>
    <select id="getByName" parameterType="string" resultType="io.github.xudaojie.jdoc.model.AccountModel">
        select * from account where username = #{username}
    </select>
    <update id="update">
        update account set nickname = #{nickname} where id = #{id}
    </update>
    <insert id="insert" useGeneratedKeys="true" keyProperty="id">
        insert into account(username, password) values (#{username}, #{password})
    </insert>
</mapper>