<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.github.xudaojie.jdoc.model.ProjectModel">
    <select id="get" parameterType="long" resultType="io.github.xudaojie.jdoc.model.ProjectModel">
        select * from project where id = #{id}
    </select>
    <select id="getListByOwner" resultType="io.github.xudaojie.jdoc.model.ProjectModel">
        select * from project where owner = #{owner}
    </select>
    <update id="update">
        update project set owner = #{owner}, creator = #{creator} where id = #{id}
    </update>
    <insert id="insert" keyProperty="id" useGeneratedKeys="true" parameterType="io.github.xudaojie.jdoc.model.ProjectModel">
        insert into project(name, owner, creator) values (#{name}, #{owner}, #{creator})
    </insert>
</mapper>